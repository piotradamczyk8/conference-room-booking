# ============================================
# Conference Room Booking System - .gitignore
# ============================================

# ====================
# IDE & Editors
# ====================
.idea/
.vscode/
*.sublime-project
*.sublime-workspace
*.swp
*.swo
*~

# ====================
# OS Files
# ====================
.DS_Store
.DS_Store?
._*
.Spotlight-V100
.Trashes
ehthumbs.db
Thumbs.db
*.log

# ====================
# Environment
# ====================
.env
.env.local
.env.*.local
!.env.example

# ====================
# Backend (PHP/Symfony)
# ====================
/backend/vendor/
/backend/var/
/backend/.php-cs-fixer.cache
/backend/phpunit.xml
/backend/.phpunit.result.cache
/backend/.phpunit.cache/
/backend/public/bundles/

# Symfony specific
/backend/config/secrets/prod/prod.decrypt.private.php
/backend/public/build/

# PHPStan
/backend/phpstan.neon.dist.local

# Composer
composer.phar

# ====================
# Frontend (Node.js/Next.js)
# ====================
/frontend/node_modules/
/frontend/.next/
/frontend/out/
/frontend/.turbo/
/frontend/coverage/
/frontend/.eslintcache
/frontend/.swc/

# Next.js build output
/frontend/build/
/frontend/dist/

# Testing
/frontend/test-results/
/frontend/playwright-report/
/frontend/playwright/.cache/

# Storybook
/frontend/storybook-static/

# TypeScript
*.tsbuildinfo

# Yarn
/frontend/.yarn/*
!/frontend/.yarn/patches
!/frontend/.yarn/plugins
!/frontend/.yarn/releases
!/frontend/.yarn/sdks
!/frontend/.yarn/versions
/frontend/.pnp.*

# npm
npm-debug.log*
yarn-debug.log*
yarn-error.log*

# ====================
# Docker
# ====================
# Docker volumes data (persisted locally)
/docker/data/

# ====================
# Database
# ====================
*.sql
*.sqlite
*.sqlite3
!**/migrations/*.sql

# ====================
# Build & Dist
# ====================
/build/
/dist/

# ====================
# Temporary files
# ====================
*.tmp
*.temp
*.bak
*.backup

# ====================
# Certificates & Keys
# ====================
*.pem
*.key
*.crt
*.p12
*.pfx
!docker/nginx/ssl/.gitkeep

# ====================
# Misc
# ====================
*.cache
.cache/
.parcel-cache/

# Documentation build
/docs/_build/
